  "bugs": {
    "url": "https://github.com/colingoodale/nimbly-testbuild/issues"
  },
  "homepage": "https://github.com/colingoodale/nimbly-testbuild#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "concurrently": "^5.0.0",
    "express": "^4.17.3",
    "if-env": "^1.0.4",
    "path": "^0.12.7",
    "react-animations": "^1.0.0",
    "styled-components": "^4.4.1"
  }
}
